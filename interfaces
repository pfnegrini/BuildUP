auto lo

iface lo inet loopback
iface eth0 inet dhcp
	pre-up /usr/local/bin/upWifi.sh

auto wlan0
allow-hotplug wlan0
iface wlan0 inet dhcp
  wpa-ssid "HomeNTW2.4GHz"
  wpa-psk  "C4"(0$,NcSu^L[hT.M)X0M'm9EG:_+"
  post-down /usr/local/bin/downWifi.sh
  pre-up /usr/local/bin/upWifi.sh

